#!./common/bats/bin/bats
# -*-sh-*-

load ./common/test_helper_functions || exit 1
source ./common/functions || exit 1

if [ -s ./TEST_ENV ];then
    source ./TEST_ENV
fi

@test "[spack] check for RPM" {
    check_if_rpm_installed "spack${DELIM}"
}

@test "[spack] test executable" {
    spack test
}
